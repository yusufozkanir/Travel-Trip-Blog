@using TravelTripProje.Models.Sınıflar
@model List<Blog>

<div class="box1-left">
    @foreach (var x in Model)
    {
        <div class="bot-img">
            <img src="@x.BlokImage" alt="" />
        </div>
        <div class="bot-desc">
            <p><a href="/Blog/BlogDetay/@x.ID">@x.Aciklama.Substring(0, 75)...</a> </p>
        </div>
        <div class="clear"></div>
    }
</div>

